<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<StackPane id="pane" prefHeight="1280" prefWidth="720" xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.example.judy.controllers.InitialGameScreenController">
    <children>
        <VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>

            <Label fx:id="initialGameText"/>
            <Button text="Initial Game Screen" onAction="#onHelloButtonClick"/>
        </VBox>
    </children>
</StackPane>

